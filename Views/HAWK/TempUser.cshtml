@model IEnumerable<HAWK_v.Models.TempModel>
@{
       ViewBag.MangDno = Model.ElementAt(0).Dno;
}
<div class="row tm-content-row tm-mt-big">
    <div class="col-xl-12 col-lg-12 tm-md-12 tm-sm-12 tm-col">

        <h2>Attendance Statistics</h2>
        <br />
    </div>
</div>

<div class="row">
    <div class="col-8">
        <!-- <br />-->
        <div class="table-responsive">
            <a asp-area="" asp-controller="HAWK" asp-action="AddTemp" asp-route-dno="@Model.ElementAt(0).Dno">
                <button type="button" class="btn btn-dark" style="float: right">
                    <i class="fas fa-plus"></i>
                </button>
            </a>
            <br />
            <table style="overflow:scroll" class="table table-striped mt-4">
                <thead>
                    <tr class="tm-bg-gray">
                        <th>Name</th>
                        <th>Email</th>
                        <th>Permission Start Date</th>
                        <th>Permission End Date</th>
                        <th>edit</th>
                        <th>delete</th>
                    </tr>

                </thead>
                <tbody id="dataTable">
                    @{
                        try
                        {

                            @foreach (var item in Model)
                            {
                                if (item.Id != 0)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Name)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Email)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.PStartDate)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.PEndDate)
                                        </td>
                                        <td>
                                            <a class="btn btn-outline-dark btn-floating" asp-area="" asp-controller="HAWK" asp-action="EditTemp" asp-route-id="@item.Id" role="button">
                                                <i class='fas fa-edit' style='font-size:16px'></i>
                                            </a>
                                        </td>
                                        <td>
                                            <a class="btn btn-outline-dark btn-floating" asp-area="" asp-controller="HAWK" asp-action="DeleteTempToDB" asp-route-id="@item.Id" role="button">
                                                <i class='fas fa-trash' style='font-size:16px'></i>
                                            </a>
                                        </td>
                                    </tr>
                                }
                            }
                        }
                        catch (Exception e)
                        {

                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
    @{
        string[] dep = { "0", "https://app.powerbi.com/reportEmbed?reportId=c29ed44d-e160-4796-b265-20192249a9da&autoAuth=true&ctid=2d3194e3-1654-46bd-bae2-ad37ba11b0ae&config=eyJjbHVzdGVyVXJsIjoiaHR0cHM6Ly93YWJpLXdlc3QtZXVyb3BlLXJlZGlyZWN0LmFuYWx5c2lzLndpbmRvd3MubmV0LyJ9", "https://app.powerbi.com/reportEmbed?reportId=9f0078d9-3eec-4c01-85b6-df9a7b8b7ca2&autoAuth=true&ctid=2d3194e3-1654-46bd-bae2-ad37ba11b0ae&config=eyJjbHVzdGVyVXJsIjoiaHR0cHM6Ly93YWJpLXdlc3QtZXVyb3BlLXJlZGlyZWN0LmFuYWx5c2lzLndpbmRvd3MubmV0LyJ9", "https://app.powerbi.com/reportEmbed?reportId=1694cb1b-50f6-4a9b-a488-452256f3bc70&autoAuth=true&ctid=2d3194e3-1654-46bd-bae2-ad37ba11b0ae&config=eyJjbHVzdGVyVXJsIjoiaHR0cHM6Ly93YWJpLXdlc3QtZXVyb3BlLXJlZGlyZWN0LmFuYWx5c2lzLndpbmRvd3MubmV0LyJ9" };

    }
<div class="col-4">

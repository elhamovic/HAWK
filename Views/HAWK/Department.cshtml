@model IEnumerable<HAWK_v.Models.UserModel>
@{
    ViewBag.MangDno = Model.ElementAt(0).Dno;
}
<div class="row tm-content-row tm-mt-big">
    <div class="col-xl-12 col-lg-12 tm-md-12 tm-sm-12 tm-col">

        <h2>Attendance Statistics</h2>
        <br />
    </div>
</div>
<div class="row">
    <div class="col-4">
        <br />
        <div class="table-responsive">
            <table class="table table-striped mt-4">
                <thead>
                    <tr class="tm-bg-gray">
                        <th>Name</th>
                        <th>Role</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        if (!item.Role.Contains("Manager"))
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Name)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Role)
                                </td>
                                <td>
                                    <a class="btn btn-outline-dark btn-floating" asp-area="" asp-controller="HAWK" asp-action="DeleteTempToDB" asp-route-id="@item.Id" role="button">
                                        <i class='fas fa-trash' style='font-size:16px'></i>
                                    </a>
                                </td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
